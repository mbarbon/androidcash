<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.barbon.acash"
      android:versionCode="1"
      android:versionName="1.0">
  <application android:label="@string/app_name" android:icon="@drawable/icon">
    <activity android:name=".NewExpense"
              android:label="@string/new_expense">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".NewAccount"
              android:label="@string/new_account">
      <intent-filter>
        <action android:name="org.barbon.acash.NEW_ACCOUNT" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <activity android:name=".ExpenseList"
              android:label="@string/expense_list">
      <intent-filter>
        <action android:name="org.barbon.acash.EXPENSE_LIST" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
  </application>
</manifest> 
