<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.barbon.acash"
      android:versionCode="1"
      android:versionName="1.0">
  <application android:label="@string/app_name" android:icon="@drawable/icon">
    <activity android:name=".NewExpense">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".NewAccount">
      <intent-filter>
        <action android:name="org.barbon.acash.NEW_ACCOUNT" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <activity android:name=".ExpenseList">
      <intent-filter>
        <action android:name="org.barbon.acash.EXPENSE_LIST" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <activity android:name=".AccountList">
      <intent-filter>
        <action android:name="org.barbon.acash.ACCOUNT_LIST" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <activity android:name=".ExpenseDetail">
      <intent-filter>
        <action android:name="org.barbon.acash.EXPENSE_DETAIL" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <activity android:name=".AccountDetail">
      <intent-filter>
        <action android:name="org.barbon.acash.ACCOUNT_DETAIL" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
  </application>
</manifest> 
